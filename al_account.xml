		<primary-key name="ac_pk" location ="trans_i">
			<key_column name="bank_id"/>
		<primary-key/>
		<foreign-keys>
			<foreign-key name="ac_fk" foreign-table="al_bank">
				<refers from="bank_id" to="bank_id"/>
			<foreign-key>
		<foreign-keys/>
		<indexes/>
	</table>
